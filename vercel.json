{
  "version": 2,
  "functions": {
    "backend/main.py": {
      "runtime": "@vercel/python"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "backend/main.py"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "trailingSlash": false
}